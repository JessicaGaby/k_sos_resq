<link rel="stylesheet" href="/css/buses/listar.css">
<form action="objeto/listar/"method="get"></form>

<h1>Listado de Objetos</h1>
<div id="contenedor">
    
    <div class="row">
        {{#each lista}}
        <div id="tarjeta">
            <u><h2>Datos del Objeto</h2></u>
            <div>
                <p>{{marca}}</p>
                <p>{{modelo}}</p>
                <p>{{numero_serie}}</p>
                <p>{{numero_contacto}}</p>
                <p>{{correo_electronico}}</p>
            <div>
                <div>
                    <a href="/objeto/editar/{{id_objeto}}"><button type="button">Editar</button></a>
                </div></p>
                <div>
                    <a href="/objeto/eliminar/{{id_objeto}}"><button type="button" style="background-color: red;">Eliminar</button></a>
                </div></p>
            </div>
        </div>
        {{/each}}
    </div>
</div>
